<project name="packer-reflexfps">
    <target name="build">
        <exec failonerror="true" executable="packer">
            <env key="AWS_ACCESS_KEY" value="${AWS_ACCESS_KEY}" />
            <env key="AWS_SECRET_KEY" value="${AWS_SECRET_KEY}" />
            <arg value="build" />
            <arg value="reflexfps.json" />
        </exec>
    </target>
</project>